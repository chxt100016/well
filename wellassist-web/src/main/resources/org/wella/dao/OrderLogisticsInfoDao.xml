<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.wella.dao.OrderLogisticsInfoDao">

	<insert id="createOrderLogisticsInfoDao" parameterType="org.wella.entity.OrderLogisticsInfo">
		INSERT INTO wa_order_logistics_info(order_id,delivery_time,receive_time,contact_person,contact_phone,address)
		VALUES (#{orderId},#{deliveryTime},#{receiveTime},#{contactPerson},#{contactPhone},#{address})
	</insert>
</mapper>